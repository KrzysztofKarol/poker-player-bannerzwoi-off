{
    "name": "poker-player-typescript",
    "description": "TypeScript player from lean poker. http://leanpoker.org",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "postinstall": "npm run build",
        "start": "node out/index.js",
        "test": "jest",
        "build": "./node_modules/.bin/tsc -p ./",
        "push": "npm run test && npm run build && git push",
        "dev": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/index.ts\""
    },
    "dependencies": {
        "@types/jest": "27.4.0",
        "@types/node": "17.0.12",
        "body-parser": "^1.19.1",
        "express": "^4.17.2",
        "jest": "27.4.7",
        "nodemon": "2.0.15",
        "ts-jest": "27.1.3",
        "ts-node": "10.4.0",
        "typescript": "^4.5.4"
    },
    "engines": {
        "node": "17.1.0",
        "npm": "6.14.8"
    }
}
